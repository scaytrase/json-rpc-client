{
  "name": "scaytrase/json-rpc-client",
  "description": "JSON-RPC 2.0 Client implementation",
  "authors": [
    {
      "name": "Pavel Batanov",
      "email": "pavel@batanov.me"
    }
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "~5.5|~7.0",
    "guzzlehttp/guzzle": "~6.0",
    "paragonie/random_compat": "~1.1@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5|~5.1",
    "psr/log": "~1.0",
    "psr/cache": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "\\ScayTrase\\Api\\": "src/ScayTrase/Api/"
